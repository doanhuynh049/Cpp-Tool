# stop_rtserver -- stop benchmark RTserver

#set echo
if [ $RTVERBOSE ] ; then
  set -x
fi

# Use a remote RTserver if BENCH_SERVER_NODE is set.  SMW 11/19/94
if [ $BENCH_SERVER_NODE ] ; then
  rtserver -server_names $BENCH_SERVER_NODE -stop_clients > rtstopsrv.out
else
  rtserver -stop_clients > rtstopsrv.out
fi
